def Q1():
    numbers = [1,2,3,4,5,6,7]
    for i in range(len(numbers)):
        numbers[i]=numbers[i]*numbers[i]
    print(numbers)
def Q2():
    list1 = ['Hello ','take ']
    list2 = ['Dear','Sir']
    lst = []
    for i in range(len(list1)):
        for j in range(len(list2)):
            tmp = list1[i]+list2[j]
            lst.append(tmp)
    print(lst)

            
